
public class Q6 {

	public static void main(String[] args) {
		

	}
	public static String compression(String str){
		String compressed = "";
		int count = 0;
		
		for(int i = 0; i < str.length(); i++){
			count = 0;
			if (str.charAt(i) != str.charAt(i+1)){
				compressed += str.charAt(i) + 1;
			}
		}
		
		
		
		return compressed;
		
		
	}
}
